<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>
        <variable
            name="fragmentDeliveryDetailVM"
            type="dm.sime.com.kharetati.view.viewModels.DeliveryDetailViewModel" />
    </data>
    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginHorizontal="10dp"
                android:orientation="vertical">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="reciever name"
                    android:paddingLeft="3dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="-7dp"
                    android:textAppearance="@style/Base.TextAppearance.AppCompat.Medium"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    android:textDirection="locale"
                    android:paddingStart="3dp" />
                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:hintEnabled="false">
                    <dm.sime.com.kharetati.view.customview.CleanableEditText
                        android:id="@+id/edtReceiverName"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="start"
                        android:inputType="text"
                        android:maxLength="255"
                        android:textAlignment="viewStart"
                        android:textColor="@color/black"
                        android:textDirection="locale" />
                </com.google.android.material.textfield.TextInputLayout>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="email address"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="-7dp"
                    android:paddingLeft="3dp"
                    android:textAppearance="@style/Base.TextAppearance.AppCompat.Medium"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    android:textDirection="locale"/>
                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:hintEnabled="false">

                    <dm.sime.com.kharetati.view.customview.CleanableEditText
                        android:id="@+id/edtEmailAddress"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="start"
                        android:inputType="text"
                        android:textColor="@color/black"
                        android:textDirection="locale"
                        android:textAlignment="viewStart"
                        android:maxLength="50"
                        />
                </com.google.android.material.textfield.TextInputLayout>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="mobile no"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="-7dp"
                    android:paddingLeft="3dp"
                    android:textAppearance="@style/Base.TextAppearance.AppCompat.Medium"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    android:textDirection="locale"/>

                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:hintEnabled="false">

                    <dm.sime.com.kharetati.view.customview.CleanableEditText
                        android:id="@+id/edtMobile"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:inputType="number"
                        android:hint="mobile number 9715xxxxxxxx"
                        android:maxLength="12"
                        android:textDirection="locale"
                        android:textColor="@color/black"
                        android:textAlignment="viewStart"
                        android:gravity="start" />
                </com.google.android.material.textfield.TextInputLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="makani no"
                    android:paddingLeft="3dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="-7dp"
                    android:textAppearance="@style/Base.TextAppearance.AppCompat.Medium"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    android:textDirection="locale"/>

                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:hintEnabled="false">

                    <dm.sime.com.kharetati.view.customview.CleanableEditText
                        android:id="@+id/edtMakani"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="start"
                        android:maxLength="10"
                        android:textColor="@color/black"
                        android:inputType="number"
                        android:textDirection="locale"
                        android:textAlignment="viewStart"
                        />
                </com.google.android.material.textfield.TextInputLayout>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="emirate"
                    android:paddingLeft="3dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="-7dp"
                    android:textAppearance="@style/Base.TextAppearance.AppCompat.Medium"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    android:textDirection="locale" />

                <Spinner
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/spiEmirates"
                    android:layout_marginTop="5dp"
                    android:gravity="start"
                    android:textColor="@color/black"
                    android:textAlignment="viewStart"
                    android:textDirection="locale" >
                </Spinner>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="villa flat building no"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="-7dp"
                    android:paddingLeft="3dp"
                    android:textAppearance="@style/Base.TextAppearance.AppCompat.Medium"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    android:textDirection="locale"/>
                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:hintEnabled="false">

                    <dm.sime.com.kharetati.view.customview.CleanableEditText
                        android:id="@+id/edtVillaNumber"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="start"
                        android:inputType="text"
                        android:textColor="@color/black"
                        android:textAlignment="viewStart"
                        android:textDirection="locale"/>
                </com.google.android.material.textfield.TextInputLayout>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="building name"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="-7dp"
                    android:paddingLeft="3dp"
                    android:textAppearance="@style/Base.TextAppearance.AppCompat.Medium"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    android:textDirection="locale"/>

                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:hintEnabled="false">

                    <dm.sime.com.kharetati.view.customview.CleanableEditText
                        android:id="@+id/edtBuildingName"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="start"
                        android:inputType="text"
                        android:textColor="@color/black"
                        android:textAlignment="viewStart"
                        android:textDirection="locale" />
                </com.google.android.material.textfield.TextInputLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="street address"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="-7dp"
                    android:paddingLeft="3dp"
                    android:textAppearance="@style/Base.TextAppearance.AppCompat.Medium"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    android:textDirection="locale"/>
                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:hintEnabled="false">
                    <dm.sime.com.kharetati.view.customview.CleanableEditText
                        android:id="@+id/edtStreetAddress"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="start"
                        android:textColor="@color/black"
                        android:textAlignment="viewStart"
                        android:textDirection="locale" />
                </com.google.android.material.textfield.TextInputLayout>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="mailing address"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="-7dp"
                    android:paddingLeft="3dp"
                    android:textAppearance="@style/Base.TextAppearance.AppCompat.Medium"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    android:textDirection="locale"/>
                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:hintEnabled="false">
                    <dm.sime.com.kharetati.view.customview.CleanableEditText
                        android:id="@+id/edtAddress"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="start"
                        android:textColor="@color/black"
                        android:textAlignment="viewStart"
                        android:inputType="textPostalAddress"
                        android:maxLines="2"
                        android:textDirection="locale" />
                </com.google.android.material.textfield.TextInputLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="nearest landmark"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="-7dp"
                    android:paddingLeft="3dp"
                    android:textAppearance="@style/Base.TextAppearance.AppCompat.Medium"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    android:textDirection="locale"/>
                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:hintEnabled="false">
                    <dm.sime.com.kharetati.view.customview.CleanableEditText
                        android:id="@+id/edtLandmark"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="start"
                        android:inputType="text"
                        android:textColor="@color/black"
                        android:textAlignment="viewStart"
                        android:textDirection="locale" />
                </com.google.android.material.textfield.TextInputLayout>

                <!--<Button
                    android:id="@+id/btnSubmitDetails"
                    android:layout_width="150dp"
                    android:layout_height="40dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginBottom="20dp"
                    android:layout_gravity="center"
                    android:background="@drawable/rounded_edittext_gradient"
                    android:paddingLeft="@dimen/activity_horizontal_margin"
                    android:paddingRight="@dimen/activity_horizontal_margin"
                    android:textAppearance="@style/Base.TextAppearance.AppCompat.Medium"
                    android:text="@string/save"
                    android:textStyle="bold"
                    android:textAllCaps="false"
                    android:textColor="@color/white" />-->

            </LinearLayout>
    </androidx.core.widget.NestedScrollView>
</layout>