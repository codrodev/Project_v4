<?xml version="1.0" encoding="utf-8"?>
<layout  xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <data>
        <variable
            name="activityLoginVM"
            type="dm.sime.com.kharetati.view.viewModels.LoginViewModel" />
    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/layoutRoot"
        android:orientation="vertical"
        tools:context=".view.activities.LoginActivity">

        <ImageView
            android:id="@+id/imgBackground"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:clickable="true"
            android:visibility="gone"
            android:scaleType="fitXY"
            android:src="@mipmap/building"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />
        <ImageView
            android:id="@+id/imgsplash"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:clickable="true"
            android:layout_marginTop="-120dp"
            android:visibility="visible"
            android:scaleType="fitXY"
            android:src="@drawable/splash"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <ImageView
            android:layout_width="@dimen/splash_logo_width"
            android:layout_height="@dimen/splash_logo_height"
            android:id="@+id/logoSplash"
            android:scaleType="centerCrop"
            android:visibility="visible"
            android:layout_gravity="bottom|right"
            android:layout_marginTop="@dimen/splash_logo_margin_top"
            android:layout_marginBottom="@dimen/splash_logo_margin_bottom"
            android:layout_marginRight="@dimen/splash_logo_margin_right"
            android:src="@drawable/dm_logo_500"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|center"
            android:textSize="12sp"
            android:elevation="20dp"
            android:textStyle="bold"
            android:id="@+id/copyright_arabic"
            android:layout_marginTop="-25dp"
            android:layout_marginBottom="0dp"
            android:fontFamily="@font/dubai_font"
            android:textAlignment="center"
            android:textColor="@color/maroon_light"
            android:text="@string/copyright"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|center"
            android:textSize="12sp"
            android:elevation="20dp"
            android:textStyle="bold"
            android:id="@+id/copyright_english"
            android:layout_marginTop="-48dp"
            android:layout_marginBottom="23dp"
            android:fontFamily="@font/dubai_font"
            android:textAlignment="center"
            android:textColor="@color/maroon_light"
            android:text="حقوق الطبع والنشر © بلدية دبي 2020, جميع الحقوق محفوظة"
            />


        <dm.sime.com.kharetati.view.customview.SlantView
            android:id="@+id/slantViewLoginHeader"
            android:layout_width="match_parent"
            android:layout_marginTop="12dp"
            android:layout_height="350dp"
            android:layout_gravity="start|center_horizontal"

            />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:gravity="center_horizontal"
            android:id="@+id/layoutLogo">
            <ImageView
                android:id="@+id/imageView"
                android:layout_width="107dp"
                android:layout_height="102dp"
                android:layout_marginTop="48dp"
                android:visibility="visible"
                android:src="@drawable/kharetati_white"
                app:layout_constraintBottom_toBottomOf="@+id/imgBackground"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/slantViewLoginHeader"
                app:layout_constraintVertical_bias="0.0" />




        </LinearLayout>


        <androidx.cardview.widget.CardView
            android:id="@+id/cardLogin"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
           app:cardElevation="0dp"
           app:cardBackgroundColor="@color/transparent"
            app:layout_constraintTop_toBottomOf="@+id/slantViewLoginHeader"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent">

            <LinearLayout
                android:id="@+id/layoutParentLoginContainer"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="8dp"
                android:layout_marginEnd="8dp"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="100dp"
                    android:layout_height="50dp"
                    android:layout_gravity="center"
                    android:id="@+id/imageDubaiID"
                    android:layout_marginStart="16dp"

                    android:layout_marginEnd="16dp"
                    android:src="@drawable/myid_logo" />

            <LinearLayout
                android:id="@+id/loginContainer"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:visibility="visible"
                android:orientation="vertical">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:visibility="gone">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/username"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="4dp"
                        android:fontFamily="@font/dubai_font"
                        android:textColor="@color/login_text_color"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="*"
                        android:fontFamily="@font/dubai_font"
                        android:textColor="@color/login_text_color"
                        />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layoutDirection="locale"
                    android:orientation="horizontal">

                    <AutoCompleteTextView
                        android:id="@+id/editUserName"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/edittext_height"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="40dp"

                        android:paddingEnd="8dp"
                        android:layout_marginTop="12dp"
                        android:layoutDirection="locale"
                        android:layout_marginBottom="12dp"

                        android:clickable="true"
                        android:ems="10"
                        android:hint="@string/username"
                        android:background="@color/transparent"
                        android:singleLine="true"
                        android:focusable="true"
                        android:textAlignment="viewStart"
                        android:gravity="start|center_vertical"
                        android:fontFamily="@font/dubai_font"
                        android:inputType="textEmailAddress"
                        android:paddingLeft="0dp"
                        android:textColor="@color/login_text_color"
                        android:textSize="14sp"/>


                    <ImageView
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:layout_gravity="center_vertical"
                        android:layout_marginStart="-32dp"
                        android:layout_marginTop="8dp"
                        android:padding="3dp"
                        android:layout_marginBottom="10dp"
                        android:scaleType="centerInside"
                        android:src="@drawable/user_login_icon" />


                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginTop="-8dp"
                    android:layout_marginLeft="8dp"
                    android:layout_marginRight="8dp"
                    android:background="@color/faded_text_color"/>

                <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                    android:visibility="gone"
                    android:layout_marginTop="8dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/password"
                    android:layout_marginStart="8dp"
                    android:paddingTop="5dp"
                    android:layout_marginEnd="4dp"
                    android:fontFamily="@font/dubai_font"
                    android:textColor="@color/login_text_color"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="*"
                    android:fontFamily="@font/dubai_font"
                    android:textColor="@color/login_text_color"
                    />
            </LinearLayout>

                <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layoutDirection="locale"
                android:gravity="center_vertical"
                android:orientation="horizontal">


                <EditText
                    android:id="@+id/editPassword"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/edittext_height"
                    android:textAlignment="viewStart"
                    android:gravity="start|center_vertical"
                    android:layoutDirection="locale"
                    android:layout_marginTop="12dp"
                    android:layout_marginStart="8dp"
                    android:layout_marginEnd="40dp"
                    android:hint="@string/password"
                    android:paddingEnd="8dp"
                    android:paddingTop="8dp"
                    android:singleLine="true"

                    android:background="@color/transparent"
                    android:textColor="@color/login_text_color"
                    android:fontFamily="@font/dubai_font"
                    android:layout_marginBottom="12dp"
                    android:ems="10"
                    android:textSize="14sp"

                    android:inputType="textWebPassword"
                    android:paddingLeft="0dp"/>


                    <ImageView
                        android:layout_width="@dimen/edit_drawable_height"
                        android:layout_height="@dimen/edit_drawable_height"
                        android:layout_gravity="center_vertical"
                        android:layout_marginStart="-32dp"
                        android:layout_marginEnd="16dp"
                        android:padding="3dp"
                        android:layout_marginBottom="10dp"
                        android:layout_marginTop="8dp"
                        android:scaleType="centerInside"
                        android:src="@drawable/password" />


            </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginTop="-8dp"
                    android:layout_marginLeft="8dp"
                    android:layout_marginRight="8dp"
                    android:background="@color/faded_text_color"/>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:layoutDirection="locale"
                    android:weightSum="1"
                    android:gravity="center_vertical"
                    android:layout_gravity="center_vertical"
                    android:id="@+id/layoutRememberMe"
                    android:layout_marginBottom="8dp"
                    android:orientation="horizontal">
                    <CheckBox
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="start"
                        android:clickable="true"
                        android:gravity="center_vertical|start"
                        android:textColor="@color/faded_text_color"
                        android:id="@+id/chkRememberMe"
                        android:fontFamily="@font/dubai_font"
                        android:focusable="false"
                        android:layout_weight=".55"
                        android:textSize="13sp"
                        android:text="@string/rememberme" />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/txtForgotPassword"
                        android:layout_gravity="end"
                        android:gravity="center_vertical|end"
                        android:layout_marginStart="16dp"
                        android:layout_marginEnd="16dp"
                        android:singleLine="false"
                        android:maxLines="2"
                        android:layout_weight=".45"
                        android:fontFamily="@font/dubai_font"
                        android:text="@string/forgot_username_password"
                        android:textAlignment="viewEnd"
                        android:textSize="13sp"
                        android:textColor="@color/faded_text_color" />

                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginTop="4dp"
                    android:layout_marginLeft="8dp"
                    android:layout_marginRight="8dp"
                    android:background="@color/faded_text_color"/>

                <Button
                    android:layout_width="match_parent"
                    android:layout_height="44dp"
                    android:layout_marginStart="32dp"
                    android:layout_marginEnd="32dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginBottom="4dp"
                    android:id="@+id/btnLogin"
                    android:fontFamily="@font/dubai_font"
                    android:background="@drawable/gradient_background"
                    android:clickable="true"
                    android:focusable="true"
                    android:text="@string/login"
                    android:textAllCaps="false"
                    android:textColor="@color/white"
                    android:textSize="@dimen/login_font_size" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="4dp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/newuser"
                        android:id="@+id/newuser"
                        android:textSize="12sp"
                        android:fontFamily="@font/dubai_font"
                        android:textColor="@color/login_text_color" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="2dp"
                        android:text="@string/reg"
                        android:id="@+id/register"
                        android:textSize="12sp"
                        android:fontFamily="@font/dubai_font"
                        android:textColor="@color/maroon_light" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:id="@+id/layoutOR"
                    android:weightSum="3"
                    android:layout_marginStart="8dp"
                    android:layout_marginEnd="8dp"
                    android:orientation="horizontal">

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="2dp"
                        android:layout_marginTop="4dp"
                        android:layout_marginLeft="8dp"
                        android:layout_marginRight="8dp"
                        android:layout_weight="1"
                        android:background="@color/maroon_light"/>

                    <TextView
                        android:id="@+id/txtOR"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/dubai_font"
                        android:text="@string/or"
                        android:textAlignment="center"
                        android:layout_marginStart="8dp"
                        android:layout_margin="8dp"
                        android:textColor="@color/maroon_light"
                        android:textSize="@dimen/login_font_size" />
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="2dp"
                        android:layout_marginTop="4dp"
                        android:layout_marginLeft="8dp"
                        android:layout_marginRight="8dp"
                        android:layout_weight="1"
                        android:background="@color/maroon_light"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/layoutMyIDUAEPass"
                    android:layout_width="wrap_content"
                    android:layout_height="45dp"
                    android:layout_marginStart="32dp"
                    android:layout_marginEnd="32dp"
                    android:orientation="horizontal"
                    android:visibility="visible"
                    android:paddingEnd="16dp"
                    android:paddingStart="16dp"
                    android:background="@drawable/rounded_edittext_boarder_bg"
                    android:layout_marginTop="10dp"
                    android:gravity="center"

                    android:layout_gravity="center">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"

                        android:id="@+id/txtMyIDLoginwith"

                        android:gravity="center"

                        android:layout_gravity="center"
                        android:fontFamily="@font/dubai_font"
                        android:text="@string/login_with"

                        android:paddingEnd="2dp"
                        android:visibility="visible"
                        android:textColor="#231f20"
                        android:textSize="16sp" />
                    <ImageView
                        android:layout_width="48dp"
                        android:layout_height="44dp"
                        android:padding="5dp"
                        android:id="@+id/imgMyIDUAEPass"
                        android:scaleType="fitXY"
                        android:layout_gravity="center"
                        android:src="@drawable/uaepass" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"

                        android:id="@+id/txtMyIDUAEPass"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:fontFamily="@font/dubai_font"
                        android:text="@string/with_uaepass"

                        android:visibility="visible"
                        android:textColor="#231f20"
                        android:textSize="16sp" />
                </LinearLayout>
                <TextView
                    android:id="@+id/txtMyIDGuest"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="16dp"
                    android:layout_marginBottom="4dp"
                    android:textAllCaps="false"
                    android:fontFamily="@font/dubai_font"
                    android:text="@string/signin_as_guest"
                    android:textAlignment="center"
                    android:textColor="@color/maroon_light"
                    android:textSize="@dimen/login_font_size" />
                <dm.sime.com.kharetati.view.customview.SwitchCompatEx
                    android:id="@+id/switchLanguage"
                    android:layout_width="150dp"
                    android:layout_height="35dp"
                    android:layout_marginTop="16dp"
                    android:minHeight="35dp"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:layoutDirection="rtl"
                    android:fontFamily="@font/dubai_font"
                    android:visibility="visible"
                    app:switchMinWidth="150dp" />


                <!--<Button
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/edittext_height"
                    android:layout_marginStart="16dp"
                    android:layout_marginEnd="16dp"
                    android:layout_marginBottom="4dp"
                    android:visibility="gone"
                    android:background="@drawable/maroon_border_background"
                    android:id="@+id/btnSignUp"
                    android:fontFamily="@font/dubai_font"
                    android:text="@string/createAccount"
                    android:textAllCaps="false"
                    android:textColor="@color/textColor"
                    android:textSize="@dimen/login_font_size" />-->
            </LinearLayout>




            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:layout_gravity="center"
                android:id="@+id/layoutUae"
                android:visibility="gone"
                android:orientation="vertical">
                <LinearLayout
                    android:id="@+id/layoutEnableUAEPass"
                    android:layout_width="300dp"
                    android:layout_height="55dp"
                    android:orientation="horizontal"
                    android:visibility="visible"
                    android:background="@drawable/rounded_edittext_boarder_bg"
                    android:layout_marginTop="10dp"
                    android:gravity="center"

                    android:layout_gravity="center">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"

                        android:id="@+id/txtLoginwith"

                        android:gravity="center"

                        android:layout_gravity="center"
                        android:fontFamily="@font/dubai_font"
                        android:text="@string/login_with"

                        android:paddingEnd="2dp"
                        android:visibility="visible"
                        android:textColor="@color/black"
                        android:textSize="20sp" />
                    <ImageView
                        android:layout_width="48dp"
                        android:layout_height="44dp"
                        android:padding="5dp"
                        android:id="@+id/imgUAEPass"
                        android:scaleType="fitXY"
                        android:layout_gravity="center"
                        android:src="@drawable/uaepass" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"

                        android:id="@+id/txtUAEPass"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:fontFamily="@font/dubai_font"
                        android:text="@string/with_uaepass"


                        android:visibility="visible"
                        android:textColor="@color/black"
                        android:textSize="20sp" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:id="@+id/layoutUaeOR"
                    android:weightSum="3"
                    android:layout_marginTop="20dp"
                    android:layout_marginStart="8dp"
                    android:layout_marginEnd="8dp"
                    android:orientation="horizontal">

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="2dp"
                        android:layout_marginTop="4dp"
                        android:layout_marginLeft="8dp"
                        android:layout_marginRight="8dp"
                        android:layout_weight="1"
                        android:background="#dca8b0"/>

                    <TextView
                        android:id="@+id/txtuaeOR"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/dubai_font"
                        android:text="@string/or"
                        android:textAlignment="center"
                        android:layout_marginStart="8dp"
                        android:layout_margin="8dp"
                        android:textColor="@color/maroon_light"
                        android:textSize="@dimen/login_font_size" />
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="2dp"
                        android:layout_marginTop="4dp"
                        android:layout_marginLeft="8dp"
                        android:layout_marginRight="8dp"
                        android:layout_weight="1"
                        android:background="#dca8b0"/>
                </LinearLayout>
                <TextView
                    android:layout_width="300dp"
                    android:layout_height="55dp"
                    android:background="@drawable/rounded_edittext_maroon_bg"
                    android:id="@+id/txtGuest"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:textAllCaps="false"
                    android:layout_marginTop="20dp"
                    android:fontFamily="@font/dubai_font"
                    android:text="@string/signin_as_guest"
                    android:textAlignment="center"
                    android:textColor="@color/white"
                    android:textSize="20sp" />
                <dm.sime.com.kharetati.view.customview.SwitchCompatEx
                    android:id="@+id/switchLanguageUae"
                    android:layout_width="150dp"
                    android:layout_height="35dp"
                    android:layout_marginTop="48dp"
                    android:minHeight="35dp"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:layoutDirection="rtl"
                    android:fontFamily="@font/dubai_font"
                    android:visibility="gone"
                    app:switchMinWidth="150dp" />

            </LinearLayout>

